<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_b4b4b4"/>
            <stroke android:color="@color/grey_b4b4b4"
                android:width="1dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_bg_F7F7F7"/>
            <stroke android:color="@color/grey_bg_F7F7F7"
                android:width="1dp"/>
        </shape>
    </item>
</selector>